<script>
    let latitude = [];
    let longitude = [];
    <% for building in @buildings %>
        latitude.push(<%= building.address.latitude %>);
        longitude.push(<%= building.address.longitude %>)
    <% end %>
    
    for (let i = 0; i < latitude.length; i++){
        console.log(`longitude: ${longitude[i]}, latitude: ${latitude[i]}`);
    }
</script>