<script>
    let count = [];
<% for building in @buildings %>
    <% count = 0 %>
    <% columns = building.battery.column %>
    <% for column in columns %>
        <% count += column.elevator.count %>
    <% end %>
    count.push(<%= count %>);
<% end %>

for (let i = 0; i < count.length; i++)
    console.log(count[i]);

</script>

 

